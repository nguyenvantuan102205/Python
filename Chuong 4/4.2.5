text = input()
uppercase_count = 0
lowercase_count = 0
digit_count = 0
for char in text:
    if char.isupper(): 
        uppercase_count += 1
    elif char.islower(): 
        lowercase_count += 1
    elif char.isdigit(): 
        digit_count += 1
print(f"Số chữ cái viết hoa: {uppercase_count}")
print(f"Số chữ cái viết thường: {lowercase_count}")
print(f"Số chữ số: {digit_count}")
