def g(arr,s):
    l=0
    r=len(arr)-1
    while(l<=r):
        m=(l+r)//2
        if (arr[m]==s):
            return True
        elif (arr[m]<s):
            l=m+1
        else:
            r=m-1
    return False

n,k=map(int,input().split())
a=list(map(int,input().split()))
ds=list(map(int,input().split()))
kq=[]
for x in ds:
    if(g(a,x)):
        kq.append("YES")
    else:
        kq.append("NO")
print("\n".join(kq))
